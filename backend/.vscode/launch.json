{
  "configurations": [
    {
      "type": "debugpy",
      "request": "launch",
      "name": "Poetry: Launch FastAPI",
      "module": "uvicorn",
      "args": [
        "main:app",
        "--reload"
      ],
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "python": "${workspaceFolder}/.venv/Scripts/python.exe",
      "preLaunchTask": "poetry-install"
    },
    {
      "type": "debugpy",
      "request": "launch",
      "name": "Launch main.py",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal"
    }
  ],
}
